{
  "manifest_version": 2,

  "name": "Uplabs curator extension",
  "description": "Showcase from browser",
  "version": "0.1",

  "browser_action": {
   "default_icon": "uplabs.png",
   "default_popup": "index.html"
 },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
